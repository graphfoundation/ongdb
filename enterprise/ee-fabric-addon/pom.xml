<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <artifactId>ee-fabric-addon</artifactId>
  <build>
    <plugins>
      <plugin>
        <artifactId>scala-maven-plugin</artifactId>
        <groupId>net.alchim31.maven</groupId>
      </plugin>


    </plugins>
  </build>
  <dependencies>

    <dependency>
      <artifactId>ee-common</artifactId>
      <groupId>io.github.onograph.shared</groupId>
      <version>${project.version}</version>
    </dependency>


    <dependency>
      <artifactId>ee-ha-raft-cluster</artifactId>
      <groupId>io.github.onograph</groupId>
      <version>${project.version}</version>

    </dependency>

    <dependency>
      <artifactId>neo4j-fabric</artifactId>
      <groupId>org.neo4j</groupId>
      <version>${project.version}</version>
    </dependency>
    <dependency>
      <artifactId>annotations</artifactId>
      <groupId>org.neo4j</groupId>
      <version>${project.version}</version>
    </dependency>


    <dependency>
      <artifactId>neo4j-java-driver</artifactId>
      <groupId>org.neo4j.driver</groupId>
      <optional>true</optional>
      <scope>compile</scope>
    </dependency>


    <dependency>
      <artifactId>scala-reflect</artifactId>
      <groupId>org.scala-lang</groupId>
    </dependency>
    <dependency>
      <artifactId>junit-jupiter</artifactId>
      <groupId>org.junit.jupiter</groupId>
    </dependency>
    <dependency>
      <artifactId>assertj-core</artifactId>
      <groupId>org.assertj</groupId>
    </dependency>
    <dependency>
      <artifactId>mockito-core</artifactId>
      <groupId>org.mockito</groupId>
    </dependency>
    <dependency>
      <artifactId>junit-vintage-engine</artifactId>
      <groupId>org.junit.vintage</groupId>
    </dependency>
    <dependency>
      <artifactId>scalatest_${scala.binary.version}</artifactId>
      <groupId>org.scalatest</groupId>
    </dependency>
    <dependency>
      <artifactId>pprint_${scala.binary.version}</artifactId>
      <groupId>com.lihaoyi</groupId>
    </dependency>

  </dependencies>
  <groupId>io.github.onograph</groupId>
  <modelVersion>4.0.0</modelVersion>

  <name>Fabric Enterprise</name>

  <packaging>jar</packaging>

  <parent>
    <artifactId>ono-parent</artifactId>
    <groupId>io.github.onograph</groupId>
    <relativePath>../pom.xml</relativePath>
    <version>4.3.1-SNAPSHOT</version>
  </parent>

  <properties>
    <moduleName>io.github.onograph.fabric</moduleName>
  </properties>


  <version>4.3.1-SNAPSHOT</version>


</project>

